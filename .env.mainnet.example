NODE_ENV=production
PORT=5000
DATABASE_URL=postgres://<user>:<pass>@<host>:5432/<db>
SESSION_SECRET=<strong-random-secret>
SOLANA_CLUSTER=mainnet-beta
SOLANA_RPC_ENDPOINTS=https://api.mainnet-beta.solana.com,https://<backup-mainnet-rpc>
REWARDS_PAYOUTS_ENABLED=true
SMART_CONTRACT_ENABLED=true
SMART_CONTRACT_PROGRAM_ID=<deployed-mainnet-program-id>
SMART_CONTRACT_CLAIMS_FALLBACK_TO_TRANSFER=false
SYSTEM_WALLET_PRIVATE_KEY=<base58-private-key>
PAYOUT_FAILURE_ALERT_THRESHOLD_PERCENT=10
RPC_ERROR_ALERT_THRESHOLD_COUNT=3
