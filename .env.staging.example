NODE_ENV=production
PORT=5000
DATABASE_URL=postgres://<user>:<pass>@<host>:5432/<db_staging>
SESSION_SECRET=<strong-random-secret-staging>
SOLANA_CLUSTER=devnet
SOLANA_RPC_ENDPOINTS=https://api.devnet.solana.com,https://<backup-devnet-rpc>
REWARDS_PAYOUTS_ENABLED=false
SMART_CONTRACT_ENABLED=true
SMART_CONTRACT_PROGRAM_ID=<deployed-devnet-program-id>
SMART_CONTRACT_CLAIMS_FALLBACK_TO_TRANSFER=true
SYSTEM_WALLET_PRIVATE_KEY=<base58-private-key>
PAYOUT_FAILURE_ALERT_THRESHOLD_PERCENT=15
RPC_ERROR_ALERT_THRESHOLD_COUNT=5
